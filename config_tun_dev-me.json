      {
        "clash_mode": "Direct",
        "server": "default-dns"
      },
      {
        "clash_mode": "Global",
        "server": "google-https-dns"
      },
      {
        "rule_set": "cn-rule",
        "action": "route",
        "server": "default-dns"
