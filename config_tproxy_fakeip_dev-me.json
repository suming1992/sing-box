            },
            {
                "rule_set": "proxy-rule",
                "server": "google-https-dns"
            },
            {
                "server": "dns-fakeip",
                "rewrite_ttl": 1,
                "query_type": [
                    "A",
                    "AAAA"
                ]
            }
